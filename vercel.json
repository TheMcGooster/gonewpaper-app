{
  "headers": [
    {
      "source": "/OneSignalSDKWorker.js",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/daily-morning",
      "schedule": "0 12 * * *"
    },
    {
      "path": "/api/cron/event-reminders",
      "schedule": "0 11 * * *"
    },
    {
      "path": "/api/cron/sync-knoxville-events",
      "schedule": "0 13 * * *"
    }
  ]
}
